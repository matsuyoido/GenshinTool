{
  "name": "genshin-score",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "run-p lint:*",
    "lint:code": "biome lint src/",
    "lint:style": "biome format src/",
    "fix": "run-s fix:*",
    "fix:style": "biome format --write src/",
    "fix:code": "biome lint --apply src/",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
